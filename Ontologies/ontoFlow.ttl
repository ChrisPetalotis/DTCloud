@prefix : <http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/> .

<http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow> rdf:type owl:Ontology ;
                                                              owl:imports <http://www.semanticweb.org/VasileiosDT/ontologies/ontoScenario> ;
                                                              rdfs:label "Workflow Ontology" .

#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2000/01/rdf-schema#Literal
rdfs:Literal rdfs:label "literal" .


###  http://www.w3.org/2001/XMLSchema#string
xsd:string rdfs:label "string" .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/associatedEntity
:associatedEntity rdf:type owl:ObjectProperty ;
                  rdfs:domain :WorkflowCondition ;
                  rdfs:range prov:Entity ;
                  rdfs:label "associated Entity" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/employsAgent
:employsAgent rdf:type owl:ObjectProperty ;
              rdfs:domain prov:Agent ;
              rdfs:range prov:Agent ;
              rdfs:label "employs Agent" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/follows
:follows rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         rdfs:domain prov:Activity ;
         rdfs:range prov:Activity ;
         rdfs:label "follows" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/hasEffect
:hasEffect rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:domain prov:Activity ;
           rdfs:range :WorkflowCondition ;
           rdfs:label "has Effect" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/hasInput
:hasInput rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdfs:domain prov:Activity ;
          rdfs:range :Data ;
          rdfs:label "has Input" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/hasOutput
:hasOutput rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:domain prov:Activity ;
           rdfs:range :Data ;
           rdfs:label "has Output" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/hasPart
:hasPart rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         owl:inverseOf :isPartOf ;
         rdfs:domain :WorkFlow ;
         rdfs:range prov:Activity ;
         rdfs:label "has Part" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/hasPrecondition
:hasPrecondition rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdfs:domain prov:Activity ;
                 rdfs:range :WorkflowCondition ;
                 rdfs:label "has Precondition" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/hostsSoftwareAgent
:hostsSoftwareAgent rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf owl:topObjectProperty ;
                    rdfs:domain :Service ;
                    rdfs:range prov:SoftwareAgent ;
                    rdfs:label "hosts Software Agent" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/isPartOf
:isPartOf rdf:type owl:ObjectProperty ;
          rdfs:domain prov:Activity ;
          rdfs:range :WorkFlow ;
          rdfs:label "is Part Of" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/performs
:performs rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdfs:domain prov:Agent ;
          rdfs:range prov:Activity ;
          rdfs:label "performs" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/source
:source rdf:type owl:ObjectProperty ;
        rdfs:domain :Message ;
        rdfs:range :Service ;
        rdfs:label "source" .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/content
:content rdf:type owl:DatatypeProperty ;
         rdfs:domain :Data ;
         rdfs:range xsd:string ;
         rdfs:label "content" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/hasConfigDir
:hasConfigDir rdf:type owl:DatatypeProperty ;
              rdfs:domain :KB ;
              rdfs:label "has Config Dir" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/hasDataDir
:hasDataDir rdf:type owl:DatatypeProperty ;
            rdfs:domain :KB ;
            rdfs:label "has Data Dir" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/hasDescription
:hasDescription rdf:type owl:DatatypeProperty ;
                rdfs:range xsd:string ;
                rdfs:label "has Description" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/hasEndpoint
:hasEndpoint rdf:type owl:DatatypeProperty ;
             rdfs:domain :Service ;
             rdfs:range xsd:string ;
             rdfs:label "has Endpoint" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/hasName
:hasName rdf:type owl:DatatypeProperty ;
         rdfs:range xsd:string ;
         rdfs:label "has Name" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/hasRequirements
:hasRequirements rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf owl:topDataProperty ;
                 rdfs:domain :Service ;
                 rdfs:range xsd:string ;
                 rdfs:label "has Requirements" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/hasRootDir
:hasRootDir rdf:type owl:DatatypeProperty ;
            rdfs:domain :Service ;
            rdfs:range xsd:string ;
            rdfs:label "has Root Dir" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/hasSPARQLEndpoint
:hasSPARQLEndpoint rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf :hasEndpoint ;
                   rdfs:domain :KB ;
                   rdfs:range xsd:string ;
                   rdfs:label "has SPARQL Endpoint" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/hasType
:hasType rdf:type owl:DatatypeProperty ;
         rdfs:domain :Data ;
         rdfs:range xsd:string ;
         rdfs:label "has Type" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/hasValue
:hasValue rdf:type owl:DatatypeProperty ;
          rdfs:range rdfs:Literal ;
          rdfs:label "has Value" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/sourceCode
:sourceCode rdf:type owl:DatatypeProperty ;
            rdfs:domain prov:SoftwareAgent ;
            rdfs:range xsd:string ;
            rdfs:label "source Code" .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/DB
:DB rdf:type owl:Class ;
    rdfs:subClassOf :Service ;
    rdfs:comment "A Service that stores data related to the Digital Twin, and can be queried to retrieve those data." ;
    rdfs:label "Database" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/DBUpdate
:DBUpdate rdf:type owl:Class ;
          rdfs:subClassOf :WorkflowCondition ;
          rdfs:label "Database Update" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/Data
:Data rdf:type owl:Class ;
      rdfs:subClassOf prov:Entity ;
      rdfs:comment "Data is any kind of information that is passes from an Activtiy to another Activity. It could be anythin from a simple string to a whole pandas DataFrame. The type of Data is denoted by the hasType property." ;
      rdfs:label "Data" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/DataIngestion
:DataIngestion rdf:type owl:Class ;
               rdfs:subClassOf :WorkFlow ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasEffect ;
                                 owl:someValuesFrom :KBUpdate
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasPart ;
                                 owl:someValuesFrom :SelectQuerying
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasPart ;
                                 owl:someValuesFrom :UpdateQuerying
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasPrecondition ;
                                 owl:someValuesFrom :RunningKB
                               ] ;
               rdfs:comment "The activity of receiving data from the Physical Part, transforming them to a form that is consistent to the domain ontologies, and saving them to the Knowledge Graph." ;
               rdfs:label "Data Ingestion" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/DataIngestionAgent
:DataIngestionAgent rdf:type owl:Class ;
                    rdfs:subClassOf prov:SoftwareAgent ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :employsAgent ;
                                      owl:someValuesFrom :SPARQLQueryAgent
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :performs ;
                                      owl:someValuesFrom :DataIngestion
                                    ] ;
                    rdfs:label "Data Ingestion Agent" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/DataTransformation
:DataTransformation rdf:type owl:Class ;
                    rdfs:subClassOf prov:Activity ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :hasInput ;
                                      owl:someValuesFrom :Data
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :hasOutput ;
                                      owl:someValuesFrom :Data
                                    ] ;
                    rdfs:comment "The activity of transforming data from one form to another. Data transformation is a major part of the Data Ingestion phase in a DT. It should be noted that this Class only denotes an Activity that has transformation as the its main role. For example, transforming a CSV or a pandas DataFrame to RDF data. There could be minor data transformations within other Activities but describing them with a class was not deemed necessary." ;
                    rdfs:label "Data Transformation" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/DataTransformationAgent
:DataTransformationAgent rdf:type owl:Class ;
                         rdfs:subClassOf prov:SoftwareAgent ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :performs ;
                                           owl:someValuesFrom :DataTransformation
                                         ] ;
                         rdfs:label "Data Transformation Agent" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/ExistingUser
:ExistingUser rdf:type owl:Class ;
              rdfs:subClassOf :UserDBUpdate ;
              rdfs:label "Existing User" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/GraphVisualization
:GraphVisualization rdf:type owl:Class ;
                    rdfs:subClassOf :Visualization ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :hasPart ;
                                      owl:someValuesFrom :SelectQuerying
                                    ] ;
                    rdfs:label "Graph Visualization" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/GraphVisualizationAgent
:GraphVisualizationAgent rdf:type owl:Class ;
                         rdfs:subClassOf :VisualizationAgent ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :performs ;
                                           owl:someValuesFrom :GraphVisualization
                                         ] ;
                         rdfs:label "Graph Visualization Agent" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/IngestionService
:IngestionService rdf:type owl:Class ;
                  rdfs:subClassOf :Service ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :hostsSoftwareAgent ;
                                    owl:someValuesFrom :DataIngestionAgent
                                  ] ;
                  rdfs:label "Ingestion Service" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/KB
:KB rdf:type owl:Class ;
    rdfs:subClassOf :DB ;
    rdfs:comment "A Graph Database or Triple Store, that stores data in the RDF format and provides an endpoint that can be queried using SPARQL." ;
    rdfs:label "Knowledge Base" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/KBUpdate
:KBUpdate rdf:type owl:Class ;
          rdfs:subClassOf :DBUpdate ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :associatedEntity ;
                            owl:someValuesFrom :KB
                          ] ;
          rdfs:label "Knowledge Base Update" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/Message
:Message rdf:type owl:Class ;
         rdfs:subClassOf :Data ;
         rdfs:comment "When an Activity hasInput some Message, it denotes communication between 2 different Services. For example, the Scenario Framework Service sends a message to the Simulation Service, including the data required by the Simulation Service to run the Simulation Model." ;
         rdfs:label "Message" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/QueryAgent
:QueryAgent rdf:type owl:Class ;
            rdfs:subClassOf prov:SoftwareAgent ;
            rdfs:label "Query Agent" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/Querying
:Querying rdf:type owl:Class ;
          rdfs:subClassOf prov:Activity ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasPrecondition ;
                            owl:someValuesFrom :RunningDB
                          ] ;
          rdfs:label "Querying" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/ResultsVisualization
:ResultsVisualization rdf:type owl:Class ;
                      rdfs:subClassOf :Visualization ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :hasInput ;
                                        owl:someValuesFrom :Message
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :hasOutput ;
                                        owl:someValuesFrom :Message
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :hasPrecondition ;
                                        owl:someValuesFrom :RunningVisualizationService
                                      ] ;
                      rdfs:label "Results Visualization" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/ResultsVisualizationAgent
:ResultsVisualizationAgent rdf:type owl:Class ;
                           rdfs:subClassOf :VisualizationAgent ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty :performs ;
                                             owl:someValuesFrom :ResultsVisualization
                                           ] ;
                           rdfs:label "Results Visualization Agent" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/RunningDB
:RunningDB rdf:type owl:Class ;
           rdfs:subClassOf :WorkflowCondition ;
           rdfs:label "Running Database" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/RunningKB
:RunningKB rdf:type owl:Class ;
           rdfs:subClassOf :RunningDB ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :associatedEntity ;
                             owl:someValuesFrom :KB
                           ] ;
           rdfs:label "Running Knowledge Base" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/RunningSimulationService
:RunningSimulationService rdf:type owl:Class ;
                          rdfs:subClassOf :WorkflowCondition ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty :associatedEntity ;
                                            owl:someValuesFrom :SimulationService
                                          ] ;
                          rdfs:label "Running Simulation Service" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/RunningVisualizationService
:RunningVisualizationService rdf:type owl:Class ;
                             rdfs:subClassOf :WorkflowCondition ,
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty :associatedEntity ;
                                               owl:someValuesFrom :VisualizationService
                                             ] ;
                             rdfs:label "Running Visualization Service" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/SPARQLQueryAgent
:SPARQLQueryAgent rdf:type owl:Class ;
                  rdfs:subClassOf :QueryAgent ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :performs ;
                                    owl:someValuesFrom :SPARQLQuerying
                                  ] ;
                  rdfs:label "SPARQL Query Agent" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/SPARQLQuerying
:SPARQLQuerying rdf:type owl:Class ;
                rdfs:subClassOf :Querying ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasInput ;
                                  owl:someValuesFrom :Data
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasPrecondition ;
                                  owl:someValuesFrom :RunningKB
                                ] ;
                rdfs:comment "The Activity of querying the Knowledge Base using SPARQL. A message with the content of the variables that need to be queried, as well as the SPARQL endpoint of the Knowledge Base is required as input. Also there is the precondition that the Knowledge Base is in Running State." ;
                rdfs:label "SPARQL Querying" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/ScenarioFramework
:ScenarioFramework rdf:type owl:Class ;
                   rdfs:subClassOf :Service ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hostsSoftwareAgent ;
                                     owl:someValuesFrom :SPARQLQueryAgent
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hostsSoftwareAgent ;
                                     owl:someValuesFrom <http://www.semanticweb.org/VasileiosDT/ontologies/ontoScenario/ScenarioConfigurationAgent>
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hostsSoftwareAgent ;
                                     owl:someValuesFrom <http://www.semanticweb.org/VasileiosDT/ontologies/ontoScenario/ScenarioExecutionAgent>
                                   ] ;
                   rdfs:comment "A Service for What-if Analysis, where alternative scenarios can be configured, executed, and compared." ;
                   rdfs:label "Scenario Framework" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/SelectQuerying
:SelectQuerying rdf:type owl:Class ;
                rdfs:subClassOf :SPARQLQuerying ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasOutput ;
                                  owl:someValuesFrom :Message
                                ] ;
                rdfs:comment "A SELECT SPARQL Query is the activity of connecting to the Knowledge Base and running a query that receives information from the Knowledge Graph. This information is the output of the Activity, which is of type Message to denote communication between two Services." ;
                rdfs:label "SELECT Querying" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/Service
:Service rdf:type owl:Class ;
         rdfs:subClassOf prov:Entity ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasDescription ;
                           owl:someValuesFrom xsd:string
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasName ;
                           owl:someValuesFrom xsd:string
                         ] ;
         rdfs:comment "A Service is a part of the DT's digital space that hosts a number of Software Agents. Following a microservice approach, each Service is autonomous and communicates with other Services through Messages." ;
         rdfs:label "Service" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/Simulation
:Simulation rdf:type owl:Class ;
            rdfs:subClassOf prov:Activity ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasInput ;
                              owl:someValuesFrom :Message
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasOutput ;
                              owl:someValuesFrom :Message
                            ] ;
            rdfs:label "Simulation" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/SimulationAgent
:SimulationAgent rdf:type owl:Class ;
                 rdfs:subClassOf prov:SoftwareAgent ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :performs ;
                                   owl:someValuesFrom :Simulation
                                 ] ;
                 rdfs:label "Simulation Agent" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/SimulationService
:SimulationService rdf:type owl:Class ;
                   rdfs:subClassOf :Service ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hostsSoftwareAgent ;
                                     owl:someValuesFrom :SimulationAgent
                                   ] ;
                   rdfs:comment "A service that uses a simulation model to simulate the behavior of a system within the digital twin." ;
                   rdfs:label "Simulation Service" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/UpdateQuerying
:UpdateQuerying rdf:type owl:Class ;
                rdfs:subClassOf :SPARQLQuerying ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasEffect ;
                                  owl:someValuesFrom :KBUpdate
                                ] ;
                rdfs:comment "A SPARQL UPDATE Query is the Activity of queyring the Knowledge Base in order to store new information to it. This new information is given to the Activity as Input and it is of type Message to denote communication between two Services. The Activity also has an effect on the Kowledge Base of type Knowledge Base Update, since the Knowledge Graph is updated with the new information." ;
                rdfs:label "UPDATE Querying" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/User
:User rdf:type owl:Class ;
      rdfs:subClassOf prov:Person ;
      rdfs:label "User" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/UserAuthentication
:UserAuthentication rdf:type owl:Class ;
                    rdfs:subClassOf prov:Activity ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :hasPrecondition ;
                                      owl:someValuesFrom :ExistingUser
                                    ] ;
                    rdfs:label "User Authentication" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/UserDB
:UserDB rdf:type owl:Class ;
        rdfs:subClassOf :DB ;
        rdfs:comment "A Database for the user credentials, such as usernames and passwords. It is usually a relational DB, such as MYSQL, PostrgeSQL, or SQLite." ;
        rdfs:label "User DB" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/UserDBQuerying
:UserDBQuerying rdf:type owl:Class ;
                rdfs:subClassOf :Querying ;
                rdfs:label "User DB Querying" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/UserDBUpdate
:UserDBUpdate rdf:type owl:Class ;
              rdfs:subClassOf :DBUpdate ;
              rdfs:label "User DB Update" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/UserRegistration
:UserRegistration rdf:type owl:Class ;
                  rdfs:subClassOf prov:Activity ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :hasEffect ;
                                    owl:someValuesFrom :ExistingUser
                                  ] ;
                  rdfs:label "User Registration" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/Visualization
:Visualization rdf:type owl:Class ;
               rdfs:subClassOf prov:Activity ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasInput ;
                                 owl:someValuesFrom :Data
                               ] ;
               rdfs:label "Visualization" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/VisualizationAgent
:VisualizationAgent rdf:type owl:Class ;
                    rdfs:subClassOf prov:SoftwareAgent ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :performs ;
                                      owl:someValuesFrom :Visualization
                                    ] ;
                    rdfs:label "Visualization Agent" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/VisualizationService
:VisualizationService rdf:type owl:Class ;
                      rdfs:subClassOf :Service ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :hostsSoftwareAgent ;
                                        owl:someValuesFrom :ResultsVisualizationAgent
                                      ] ;
                      rdfs:comment "A Service that is used for the visualization of the simulation's results." ;
                      rdfs:label "Visualization Service" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/WorkFlow
:WorkFlow rdf:type owl:Class ;
          rdfs:subClassOf prov:Activity ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasPart ;
                            owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                            owl:onClass prov:Activity
                          ] ;
          rdfs:comment "A workflow scenario describes a combination of activities in a prespecified order, so that, given some input, the desired output is achieved. The workflow also includes the agents and services that perform the activities, as well as the related conditions." ;
          rdfs:label "Workflow" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/WorkflowCondition
:WorkflowCondition rdf:type owl:Class ;
                   rdfs:subClassOf prov:Entity ;
                   rdfs:comment "A Workflow Condition is a truth that could hold at any point through a Workflow, and it could work as a precondition or an effect of some Activity. It is used to link Activities within the Workflow using the hasEffect and hasPrecondition relations. For example, an Activity that hasPrecondition some Workflow Condition could occur after an Activity that hasEffect this Workflow Condition." ;
                   rdfs:label "Workflow Condition" .


