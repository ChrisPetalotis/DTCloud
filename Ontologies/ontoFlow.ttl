###  http://www.w3.org/ns/prov#Activity
prov:Activity rdf:type owl:Class ;
              rdfs:subClassOf [ rdf:type owl:Class ;
                                owl:unionOf ( [ rdf:type owl:Restriction ;
                                                owl:onProperty :hasEffect ;
                                                owl:someValuesFrom :WorkflowCondition
                                              ]
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty :hasInput ;
                                                owl:someValuesFrom :Data
                                              ]
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty :hasOutput ;
                                                owl:someValuesFrom :Data
                                              ]
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty :hasPrecondition ;
                                                owl:someValuesFrom :WorkflowCondition
                                              ]
                                            )
                              ] ;
              rdfs:comment """An Activity is performed by an Agent and it must always have one of the following relations:
- hasInput some Data
- hasOutput some Data
- hasPrecondition some Workflow Condition
- hasEffect some Workflow Condition.

This way the Activity can be associated with other Activities to form Workflows.""" ;
              rdfs:label "Activity" .


