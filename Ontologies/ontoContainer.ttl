<http://www.semanticweb.org/VasileiosDT/ontologies/ontoContainer> rdf:type owl:Ontology ;
                                                                   rdfs:comment "This ontology describes the DT's containerization." ;
                                                                   rdfs:label "Container Ontology" .

#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2000/01/rdf-schema#Literal
rdfs:Literal rdfs:label "literal" .


###  http://www.w3.org/2001/XMLSchema#string
xsd:string rdfs:label "string" .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoContainer/connectsTo
:connectsTo rdf:type owl:ObjectProperty ;
            rdfs:domain ontoFlow:Service ;
            rdfs:range ontoFlow:Service ;
            rdfs:comment "If a Service connectsTo another Service, so that the former Service can properly operate, then the Container hosting the former Service, dependsOn the Container hosting the latter." ;
            rdfs:label "connects to" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoContainer/contains
:contains rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          owl:inverseOf :hosted_in ;
          rdfs:domain docker:Container ;
          rdfs:range ontoFlow:Service ;
          rdfs:label "contains" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoContainer/depends_on
:depends_on rdf:type owl:ObjectProperty ;
            rdfs:domain docker:Config ;
            rdfs:range docker:Container ;
            rdfs:comment "Denotes docker-compose container startup dependency." ;
            rdfs:label "depends on" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoContainer/exposesPort
:exposesPort rdf:type owl:ObjectProperty ;
             rdfs:domain docker:NetworkSettings ;
             rdfs:range :ContainerPort ;
             rdfs:label "exposes port" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoContainer/hasEnvVar
:hasEnvVar rdf:type owl:ObjectProperty ;
           rdfs:domain :Environment ;
           rdfs:range :EnvVar ;
           rdfs:label "has Environment Variable" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoContainer/hosted_in
:hosted_in rdf:type owl:ObjectProperty ;
           rdfs:domain ontoFlow:Service ;
           rdfs:range docker:Container ;
           rdfs:label "hosted in" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoContainer/mapsTo
:mapsTo rdf:type owl:ObjectProperty ;
        rdfs:domain :ContainerPort ;
        rdfs:range :HostPort ;
        rdfs:label "maps to" .


###  https://w3.org/ns/bde/docker#config
docker:config rdf:type owl:ObjectProperty ;
              rdfs:domain docker:Container ,
                          docker:Image ;
              rdfs:range docker:Config ;
              rdfs:label "config" .


###  https://w3.org/ns/bde/docker#dockerfile
docker:dockerfile rdf:type owl:ObjectProperty ;
                  rdfs:domain docker:Image ;
                  rdfs:range :Dockerfile ;
                  rdfs:label "dockerfile" .


###  https://w3.org/ns/bde/docker#env
docker:env rdf:type owl:ObjectProperty ;
           rdfs:domain docker:Config ;
           rdfs:range docker:Env ;
           rdfs:label "env" .


###  https://w3.org/ns/bde/docker#image
docker:image rdf:type owl:ObjectProperty ;
             rdfs:domain docker:Container ;
             rdfs:range docker:Image ;
             rdfs:label "image" .


###  https://w3.org/ns/bde/docker#mount
docker:mount rdf:type owl:ObjectProperty ;
             rdfs:domain docker:Container ;
             rdfs:range docker:Mounts ;
             rdfs:label "mount" .


###  https://w3.org/ns/bde/docker#network
docker:network rdf:type owl:ObjectProperty ;
               rdfs:domain docker:NetworkSettings ;
               rdfs:range docker:Networks ;
               rdfs:label "network" .


###  https://w3.org/ns/bde/docker#networkSettings
docker:networkSettings rdf:type owl:ObjectProperty ;
                       rdfs:domain docker:Container ;
                       rdfs:range docker:NetworkSettings ;
                       rdfs:label "networkSettings" .


###  https://w3.org/ns/bde/docker#state
docker:state rdf:type owl:ObjectProperty ;
             rdfs:domain docker:Container ;
             rdfs:range docker:State ;
             rdfs:label "state" .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoContainer/hasName
:hasName rdf:type owl:DatatypeProperty ;
         owl:equivalentProperty docker:name ;
         rdfs:range xsd:string ;
         rdfs:label "has name" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoContainer/hasValue
:hasValue rdf:type owl:DatatypeProperty ;
          rdfs:range rdfs:Literal ;
          rdfs:label "has value" .


###  https://w3.org/ns/bde/docker#architecture
docker:architecture rdf:type owl:DatatypeProperty ;
                    rdfs:domain docker:Image ;
                    rdfs:range xsd:string ;
                    rdfs:label "Image Architecture" .


###  https://w3.org/ns/bde/docker#cmd
docker:cmd rdf:type owl:DatatypeProperty ;
           rdfs:domain docker:Config ;
           rdfs:range xsd:string ;
           rdfs:label "Docker-Compose Command" .


###  https://w3.org/ns/bde/docker#created
docker:created rdf:type owl:DatatypeProperty ;
               rdfs:range xsd:string ;
               rdfs:comment "Denotes when the Container was created and corresponds to the Created attribute of Docker inspect." ;
               rdfs:label "created" .


###  https://w3.org/ns/bde/docker#destination
docker:destination rdf:type owl:DatatypeProperty ;
                   rdfs:domain docker:Mounts ;
                   rdfs:range xsd:string ;
                   rdfs:label "mount Destination" .


###  https://w3.org/ns/bde/docker#entrypoint
docker:entrypoint rdf:type owl:DatatypeProperty ;
                  rdfs:domain docker:Config ;
                  rdfs:range xsd:string ;
                  rdfs:label "Docker Entrypoint" .


###  https://w3.org/ns/bde/docker#id
docker:id rdf:type owl:DatatypeProperty ;
          rdfs:domain docker:Container ,
                      docker:Image ;
          rdfs:range xsd:string ;
          rdfs:label "Docker ID" .


###  https://w3.org/ns/bde/docker#name
docker:name rdf:type owl:DatatypeProperty ;
            rdfs:range xsd:string ;
            rdfs:comment "Denotes the name of a Container and corresponds to the Name attribute of Docker inspect." ;
            rdfs:label "Container Name" .


###  https://w3.org/ns/bde/docker#os
docker:os rdf:type owl:DatatypeProperty ;
          rdfs:domain docker:Image ;
          rdfs:range xsd:string ;
          rdfs:label "image OS" .


###  https://w3.org/ns/bde/docker#size
docker:size rdf:type owl:DatatypeProperty ;
            rdfs:domain docker:Image ;
            rdfs:range xsd:string ;
            rdfs:label "image Size" .


###  https://w3.org/ns/bde/docker#source
docker:source rdf:type owl:DatatypeProperty ;
              rdfs:domain docker:Mounts ;
              rdfs:range xsd:string ;
              rdfs:label "mount Source" .


###  https://w3.org/ns/bde/docker#status
docker:status rdf:type owl:DatatypeProperty ;
              rdfs:domain docker:State ;
              rdfs:range xsd:string ;
              rdfs:label "status" .


###  https://w3.org/ns/bde/docker#tags
docker:tags rdf:type owl:DatatypeProperty ;
            rdfs:domain docker:Image ;
            rdfs:range xsd:string ;
            rdfs:label "Docker Image Tags" .


###  https://w3.org/ns/bde/docker#workingDir
docker:workingDir rdf:type owl:DatatypeProperty ;
                  rdfs:domain docker:Config ;
                  rdfs:range xsd:string ;
                  rdfs:label "Docker workingDir" .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoContainer/ContainerPort
:ContainerPort rdf:type owl:Class ;
               rdfs:subClassOf :Port ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :mapsTo ;
                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass :HostPort
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasValue ;
                                 owl:someValuesFrom xsd:string
                               ] ;
               rdfs:label "Container Port" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoContainer/Dockerfile
:Dockerfile rdf:type owl:Class ;
            rdfs:label "Dockerfile" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoContainer/EnvVar
:EnvVar rdf:type owl:Class ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :hasName ;
                          owl:someValuesFrom xsd:string
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasValue ;
                          owl:someValuesFrom rdfs:Literal
                        ] ;
        rdfs:label "Environment Variable" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoContainer/Environment
:Environment rdf:type owl:Class ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty :hasEnvVar ;
                               owl:someValuesFrom :EnvVar
                             ] ;
             rdfs:label "Environment" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoContainer/HostPort
:HostPort rdf:type owl:Class ;
          rdfs:subClassOf :Port ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasValue ;
                            owl:someValuesFrom xsd:string
                          ] ;
          rdfs:label "Host Port" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoContainer/Port
:Port rdf:type owl:Class ;
      rdfs:label "Port" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoDeploy/Image
<http://www.semanticweb.org/VasileiosDT/ontologies/ontoDeploy/Image> rdf:type owl:Class ;
                                                                     rdfs:label "Image" .


###  http://www.semanticweb.org/VasileiosDT/ontologies/ontoFlow/Service
ontoFlow:Service rdf:type owl:Class ;
                 rdfs:label "Service" .


###  https://w3.org/ns/bde/docker#Config
docker:Config rdf:type owl:Class ;
              rdfs:subClassOf [ rdf:type owl:Restriction ;
                                owl:onProperty docker:env ;
                                owl:someValuesFrom docker:Env
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty docker:cmd ;
                                owl:someValuesFrom xsd:string
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty docker:workingDir ;
                                owl:someValuesFrom xsd:string
                              ] ;
              rdfs:comment "The configuration parameters of a docker container or image." ;
              rdfs:label "Docker Config" .


###  https://w3.org/ns/bde/docker#Container
docker:Container rdf:type owl:Class ;
                 rdfs:subClassOf [ rdf:type owl:Restriction ;
                                   owl:onProperty :contains ;
                                   owl:someValuesFrom ontoFlow:Service
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty docker:config ;
                                   owl:someValuesFrom docker:Config
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty docker:mount ;
                                   owl:someValuesFrom docker:Mounts
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty docker:networkSettings ;
                                   owl:someValuesFrom docker:NetworkSettings
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty docker:state ;
                                   owl:someValuesFrom docker:State
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty docker:created ;
                                   owl:someValuesFrom xsd:string
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty docker:id ;
                                   owl:someValuesFrom xsd:string
                                 ] ;
                 rdfs:label "Docker Container" .


###  https://w3.org/ns/bde/docker#Env
docker:Env rdf:type owl:Class ;
           rdfs:subClassOf :Environment ;
           rdfs:comment "The Environment parameters of a docker container or image." ;
           rdfs:label "Docker Environment" .


###  https://w3.org/ns/bde/docker#Image
docker:Image rdf:type owl:Class ;
             rdfs:subClassOf <http://www.semanticweb.org/VasileiosDT/ontologies/ontoDeploy/Image> ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty docker:dockerfile ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass :Dockerfile
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty docker:tags ;
                               owl:someValuesFrom xsd:string
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty docker:id ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onDataRange xsd:string
                             ] ;
             rdfs:label "Docker Image" .


###  https://w3.org/ns/bde/docker#Mounts
docker:Mounts rdf:type owl:Class ;
              rdfs:subClassOf [ rdf:type owl:Restriction ;
                                owl:onProperty docker:destination ;
                                owl:someValuesFrom xsd:string
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty docker:source ;
                                owl:someValuesFrom xsd:string
                              ] ;
              rdfs:label "Docker Mount" .


###  https://w3.org/ns/bde/docker#NetworkSettings
docker:NetworkSettings rdf:type owl:Class ;
                       rdfs:subClassOf [ rdf:type owl:Restriction ;
                                         owl:onProperty :exposesPort ;
                                         owl:someValuesFrom :ContainerPort
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty docker:network ;
                                         owl:someValuesFrom docker:Networks
                                       ] ;
                       rdfs:label "Docker Network Settings" .


###  https://w3.org/ns/bde/docker#Networks
docker:Networks rdf:type owl:Class ;
                rdfs:subClassOf [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasName ;
                                  owl:someValuesFrom xsd:string
                                ] ;
                rdfs:label "Docker Network" .


###  https://w3.org/ns/bde/docker#State
docker:State rdf:type owl:Class ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty docker:status ;
                               owl:someValuesFrom xsd:string
                             ] ;
             rdfs:label "Container State" .


